<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1px"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">

        <shape>
            <!-- 描边 -->
            <stroke
                android:width="1dp"
                android:color="@color/color_proving"
                android:dashGap="2dp"
                android:dashWidth="2dp"/>
            <!-- 填充 -->
            <solid android:color="@color/color_white"/>
        </shape>
    </item>
</layer-list>